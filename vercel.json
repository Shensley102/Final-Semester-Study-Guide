{
  "version": 2,
  "builds": [{ "src": "study_tool.py", "use": "@vercel/python" }],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/mobile", "dest": "/study_tool.py" },
    { "src": "/modules(.*)", "dest": "/study_tool.py" },
    { "src": "/(.*)\\.json", "dest": "/study_tool.py" },
    { "src": "/api/(.*)", "dest": "/study_tool.py" },
    { "src": "/(.*)", "dest": "/study_tool.py" }
  ]
}
