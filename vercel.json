{
  "version": 2,
  "functions": {
    "study_tool.py": { "runtime": "python3.11" }
  },
  "builds": [
    { "src": "study_tool.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "^/static/(.*)$", "dest": "/static/$1" },
    { "src": "^/(.*\\.json)$", "dest": "/$1" },
    { "src": "^/favicon\\.(ico|png)$", "dest": "/study_tool.py" },
    { "src": ".*", "dest": "/study_tool.py" }
  ]
}
